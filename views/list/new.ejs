<form action="/list/create" method="POST">
  <div class="card card-login mx-auto mt-5">
      <div class="card-header">
        Dodaj nową listę
      </div>
      <div class="card-body">
        <form>
          <div class="form-group">
            <label for="nazwa">Nazwa</label>
            <input type="text" id="nazwa" class="form-control" placeholder="Nazwa ..." name="name" />
            <input type="hidden" value="<%= req.session.me %>" name="owner" />
          </div>
          <input class="btn btn-primary btn-block" type="submit" value="Dodaj" />
        </form>
      </div>
    </div>
</form>
